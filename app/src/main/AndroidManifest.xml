<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ynu.com.ynubbs">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:label="注册" />
        <activity android:name=".Main" />
        <activity android:name=".Main_Type" />
        <activity android:name=".Main_Userinfo" />
        <activity android:name=".Post_Type" />
        <activity android:name=".Post_Publish" />
        <activity android:name=".Post_Display" />
        <activity android:name=".Reply_Submission" />
        <activity android:name=".AllPost_Display" />
        <activity android:name=".AllReply_Submission"></activity>
    </application>

</manifest>